{
  "name": "inventory-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "set NODE_ENV=HOMOLOGATION && node ./dist/src/index.js",
    "dev": "set NODE_ENV=HOMOLOGATION && nodemon",
    "swagger": "set NODE_ENV=HOMOLOGATION && ts-node ./swagger.ts",
    "test": "set NODE_ENV=TEST && vitest",
    "migrate:create": "npx typeorm migration:create",
    "homologation:migrate:generate": "set NODE_ENV=HOMOLOGATION && npx typeorm-ts-node-commonjs --dataSource ./src/database/data-source.ts migration:generate",
    "homologation:migrate": "set NODE_ENV=HOMOLOGATION && npx typeorm-ts-node-commonjs migration:run --dataSource ./src/database/data-source.ts",
    "homologation:migrate:revert": "set NODE_ENV=HOMOLOGATION && npx typeorm-ts-node-commonjs migration:revert --dataSource ./src/database/data-source.ts",
    "test:migrate:generate": "set NODE_ENV=TEST && npx typeorm-ts-node-commonjs --dataSource ./src/database/data-source-test.ts migration:generate",
    "test:migrate": "set NODE_ENV=TEST && npx typeorm-ts-node-commonjs migration:run --dataSource ./src/database/data-source-test.ts",
    "test:migrate:revert": "set NODE_ENV=TEST && npx typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "body-parser": "1.20.3",
    "concurrently": "9.0.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "joi": "17.13.3",
    "reflect-metadata": "0.2.2",
    "sqlite3": "5.1.7",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/joi": "17.2.2",
    "@types/node": "22.6.1",
    "@types/swagger-ui-express": "4.1.7",
    "eslint": "9.11.1",
    "globals": "15.9.0",
    "nodemon": "3.1.7",
    "swagger-autogen": "2.23.7",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0",
    "vitest": "2.1.1"
  }
}
